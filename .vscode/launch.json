    "version": "0.2.0",
    "configurations": [
        {
            "name": "catalog-service - Delve into  Docker",
            "type": "go",
            "request": "attach",
            "mode": "remote",
            "debugAdapter": "dlv-dap",
            "substitutePath": [
                {
                    "from": "${workspaceFolder}/back-end/catalog-service",
                    "to": "/app/",
                },
            ],
            "port": 2345,
            "host": "127.0.0.1",
            "showLog": true,
            "apiVersion": 2,
            "trace": "verbose"
        },
    ]
}